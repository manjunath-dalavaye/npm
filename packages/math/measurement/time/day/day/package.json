{
  "name": "@ilihub/day",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "TypeScript functions for converting day to other time units.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://code.ilihub.tech",
  "funding": "https://opencollective.com/ilihub",
  "repository": {
    "type": "git",
    "url": "https://github.com/ilihub/npm.git"
  },
  "bugs": {
    "url": "https://github.com/ilihub/npm/issues",
    "email": "contact@ilihub.tech"
  },
  "author": {
    "name": "ilihub",
    "email": "contact@ilihub.tech",
    "url": "https://github.com/ilihub"
  },
  "keywords": [
    "ilihub",
    "code",
    "math",
    "time",
    "millisecond",
    "second",
    "minute",
    "hour",
    "day",
    "week",
    "month",
    "year",
    "decade",
    "century",
    "tools",
    "typescript",
    "functions",
    "utils"
  ],
  "tsup": {
    "treeshake": true,
    "splitting": true,
    "entry": [
      "src/index.ts"
    ],
