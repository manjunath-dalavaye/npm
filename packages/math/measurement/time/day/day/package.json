    "@ilihub/day-to-week": "workspace:*",
    "@ilihub/day-to-month": "workspace:*",
    "@ilihub/day-to-year": "workspace:*",
    "@ilihub/day-to-decade": "workspace:*",
    "@ilihub/day-to-century": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
