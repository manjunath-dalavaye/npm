{
  "name": "@ilihub/shape",
  "version": "1.0.9",
  "publishConfig": {
    "access": "public"
  },
  "description": "A collection of useful TypeScript functions and utilities for mathematical operations on geometric shapes.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "homepage": "https://code.ilihub.tech",
  "funding": "https://opencollective.com/ilihub",
  "repository": {
    "type": "git",
    "url": "https://github.com/ilihub/npm.git"
  },
  "bugs": {
    "url": "https://github.com/ilihub/npm/issues",
    "email": "contact@ilihub.tech"
  },
  "author": {
    "name": "ilihub",
    "email": "contact@ilihub.tech",
    "url": "https://github.com/ilihub"
  },
  "keywords": [
    "ilihub",
    "code",
    "math",
    "geometry",
    "shape",
    "triangle",
    "circle",
    "cone",
    "cube",
    "cuboid",
    "cylinder",
    "semi-circle",
    "rectangle",
    "parallelogram",
    "square",
    "rhombus",
    "sphere",
    "tools",
    "typescript",
    "functions",
    "utils"
  ],
  "tsup": {
    "treeshake": true,
    "splitting": true,
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs"
    ],
    "dts": true,
    "minify": true,
    "clean": true
  },
  "scripts": {
    "lint": "eslint \"**/*.ts*\"",
    "build": "tsup",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "test": "jest --config jestconfig.json --maxWorkers=50% --passWithNoTests"
  },
  "dependencies": {
    "@ilihub/circle": "workspace:*",
    "@ilihub/cone": "workspace:*",
    "@ilihub/cube": "workspace:*",
    "@ilihub/cuboid": "workspace:*",
    "@ilihub/cylinder": "workspace:*",
    "@ilihub/parallelogram": "workspace:*",
    "@ilihub/rectangle": "workspace:*",
    "@ilihub/rhombus": "workspace:*",
    "@ilihub/semi-circle": "workspace:*",
    "@ilihub/sphere": "workspace:*",
    "@ilihub/square": "workspace:*",
    "@ilihub/triangle": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "eslint-config-custom": "workspace:*",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsconfig": "workspace:*",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
